{
  "name": "weather-web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "fix-user-agent": "sed -i '/User-Agent/d' ./src/client/configuration.ts",
    "gen-client": "openapi-generator-cli generate -i schema.yml -g typescript-axios -o ./src/client",
    "gen-fix-client": "npm run gen-client && npm run fix-user-agent",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
